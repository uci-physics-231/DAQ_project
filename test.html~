	<script language="javascript">
	  function takeValue (val) {
	  if (val > 20) {
	       var str=val.toString();
	       return str.fontcolor("red");
	  }
	  else if (val < 20 && val > 15){
	     var str=val.toString();
	     return str.fontcolor("yellow");	     
	     }
	  else {
	    var str=val.toString();	     
	    return str.fontcolor("black");
	    }
	  <!--return str;-->
	  } 

	
	</script> 
